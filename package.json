{
  "name": "radweb-app",
  "version": "1.0.13",
  "scripts": {
    "ng": "ng",
    "ng-serve": "ng serve --port 4201",
    "ng-serve-noam-ip-10.0.0.6": "ng serve --port 4201 --host 10.0.0.6",
    "test": "ng test core",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "app-server-watch": "tsc -p tsconfig.server.json --watch",
    "build": "ng build core && npm run schemas:build && npm run copy:styles && npm run server-build && npm run server-postgres-build",
    "copy:styles": "copyfiles -V -u 2  projects/core/input-styles.scss dist/core/",
    "schemas:build": "tsc -p projects/core/tsconfig.schematics.json",
    "schemas:copy:schemas": "copyfiles -V -u 2  projects/core/schematics/*/schema.json dist/core/",
    "schemas:copy:files": "copyfiles -a -p -V -u 2 projects/core/schematics/*/files/** dist/core/",
    "schemas:copy:collection": "copyfiles -V -u 3 projects/core/schematics/collection.json dist/core/schematics/",
    "postschemas:build": "npm run schemas:copy:schemas && npm run schemas:copy:files && npm run schemas:copy:collection",
    "server-build": "tsc -p projects/server/tsconfig.json && copyfiles -u 2 projects/server/package.json dist/server",
    "server-postgres-build": "tsc -p projects/server-postgres/tsconfig.json && copyfiles -u 2 projects/server-postgres/package.json dist/server-postgres",
    "publish": " npm run build && npm run patch-versions && npm run build && cd dist/core && npm publish  --access public --tag pre-release && cd.. && cd server && npm publish  --access public --tag pre-release && cd.. && cd server-postgres &&  npm publish  --access public --tag pre-release",
    "release": " npm run build && npm run patch-versions && npm run build && cd dist/core && npm publish  --access public && cd.. && cd server && npm publish  --access public  && cd.. && cd server-postgres &&  npm publish  --access public",
    "patch-versions": "cd projects/core && npm version patch && cd.. && cd server && npm version patch && cd.. && cd server-postgres && npm version patch && cd.. && cd.. && npm version patch",
    "docs": "c:/repos/radweb/docs-builder/buildsite.exe ./docs-md ./docs",
    "publish-radweb-local": " npm run radweb-build && cd dist/radweb && npm publish --registry http://localhost:4873",
    "publish-radweb-server-local": "cd dist/radweb-server && npm publish --registry http://localhost:4873 --access public",
    "publish-radweb-server-postgres-local": "cd dist/radweb-server-postgres && npm publish --registry http://localhost:4873 --access public",
    "node-serve": "./node_modules/.bin/tsc-watch -p tsconfig.server.json --onSuccess \"npm run server:dev-run",
    "build-server-watch": "tsc -p tsconfig.server.json --watch",
    "server:dev-run": "node --inspect dist-server/src/app/server/server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.9",
    "@angular/cdk": "^7.3.4",
    "@angular/common": "^7.2.9",
    "@angular/compiler": "^7.2.9",
    "@angular/core": "^7.2.9",
    "@angular/forms": "^7.2.9",
    "@angular/http": "^7.2.9",
    "@angular/material": "^7.3.4",
    "@angular/platform-browser": "^7.2.9",
    "@angular/platform-browser-dynamic": "^7.2.9",
    "@angular/router": "^7.2.9",
    "@auth0/angular-jwt": "^3.0.0",
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mssql": "^4.0.10",
    "@types/pg": "^7.4.11",
    "core-js": "^2.5.4",
    "dotenv": "^8.1.0",
    "express": "^4.16.4",
    "express-force-https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mssql": "^4.2.3",
    "password-hash": "^1.2.2",
    "pg": "^7.6.1",
    "rxjs": "~6.3.3",
    "uuid": "^3.3.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "^7.3.5",
    "@angular/compiler-cli": "^7.2.9",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/websql": "0.0.27",
    "codelyzer": "~4.5.0",
    "copyfiles": "^2.1.0",
    "cp": "^0.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "ts-node-dev": "^1.0.0-pre.30",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "tsc-watch": "^4.0.0"
  }
}
