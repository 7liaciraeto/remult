{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "outDir": "../../dist/server",
        "baseUrl": ".",
        "declaration": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "skipLibCheck": true,
        "lib": [
            "es2018",
            "dom"
        ],
        "paths": {
            "@remult/core": [
                "../../dist/core"
            ]
        }
    }
}