{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": false,
    "stripInternal": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": ".",
    "outDir": "distServer",
    "sourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "paths": {
      "radweb": [
        "dist/radweb"
      ],
      "radweb/*": [
        "dist/radweb/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "strictMetadataEmit": true
  }
}