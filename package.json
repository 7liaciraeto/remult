{
  "name": "radweb",
  "version": "0.1.6",
  "description": "radweb",
  "license": "MIT",
  "main": "index.js",
  "author": {
    "name": "Noam Honig"
  },
  "scripts": {
    "package": "rollup -c",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "server": " ts-node  -P tsconfig.server.json src/app/server/server.ts",
    "server:watch": "concurrently \"tsc -P tsconfig.server.json -w\" \"nodemon dist/src/app/server/server.js\"",
    "e2e": "ng e2e",
    "myBuild": "tsc",
    "myTest:build": "tsc -p src/tsconfig.mytest.json",
    "myTest": "npm run myTest:build && jasmine dist/src/utils/newTests.spec.js",
    "myTest:watch": "watch src/utils/newTests.spec.ts -c \"npm run myTest\"",
    "build": "ngc -p tsconfig.release.json",
    "addVersion": "npm version patch",
    "copyPackage": "copyfiles package.json dist",
    "publish-to-npm": "npm run build && npm run addVersion && npm run copyPackage && npm run package && cd dist && npm publish"
  },
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.9",
    "@angular/compiler": "^4.2.4",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/express": "^4.0.39",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.2.0",
    "concurrently": "^3.5.1",
    "copyfiles": "^1.2.0",
    "express": "^4.16.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.12.1",
    "protractor": "~5.1.2",
    "rollup": "^0.50.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "^2.3.4",
    "uglify-js": "^3.1.6"
  }
}
