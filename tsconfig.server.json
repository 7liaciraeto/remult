{
  "compilerOptions": {
    
    "declaration": false,"baseUrl": ".",
    "stripInternal": true,
    "experimentalDecorators": true,

    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": ".",
    "outDir": "distServer",
    "sourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "lib": [
      "es2015",
      "dom"
    ],
    
    "paths": {
      "radweb": [
        "projects/radweb/src/public_api"
      ],
      "radweb-server":[
        "projects/radweb-server"
      ],
      "radweb-server-postgres":[
        "projects/radweb-server-postgres/PostgresDataProvider"
      ],
    
      "radweb/*": [
        "projects/radweb/src/*"
      ]
    }
  },
  "exclude": ["projects/radweb-server-mssql/SQLServerDataProvider.ts"],
  "angularCompilerOptions": {
    "strictMetadataEmit": true
  }
}