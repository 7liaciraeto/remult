{
  "name": "radweb",
  "version": "1.1.25",
  "description": "radweb",
  "license": "MIT",
  "main": "index.js",
  "author": {
    "name": "Noam Honig"
  },
  "scripts": {
    "package": "rollup -c",
    "start": "concurrently \"ng serve --port 4201\" \"npm run server:watch\" \"ng test\"",
    "test": "ng test",
    "lint": "ng lint",
    "ng-serve": "ng serve",
    "server": " ts-node   -P tsconfig.server.json src/app/server/server.ts",
    "server:debug": " ts-node  --inspect  --debug-brk -P tsconfig.server.json src/app/server/server.ts",
    "server:watch": "concurrently \"tsc -P tsconfig.server.json -w\" \"nodemon distServer/src/app/server/server.js --ignore /appData",
    "e2e": "ng e2e",
    "myBuild": "tsc",
    "myTest:build": "tsc -p src/tsconfig.mytest.json",
    "myTest": "npm run myTest:build && jasmine dist/src/utils/newTests.spec.js",
    "myTest:watch": "watch src/utils/newTests.spec.ts -c \"npm run myTest\"",
    "build": "ngc -p tsconfig.release.json",
    "addVersion": "npm version patch",
    "copyPackage": "copyfiles package.json dist",
    "copyToEnvWeb": "copyfiles -u 1 ./dist/* ../env.web/frontend/node_modules/radweb/ && copyfiles -u 1  ./dist/utils/* ../env.web/frontend/node_modules/radweb/ && copyfiles -u 1  ./dist/utils/angular/* ../env.web/frontend/node_modules/radweb/ && copyfiles -u 1  ./dist/bundles/* ../env.web/frontend/node_modules/radweb/",
    "copyToStarterKit": "copyfiles -u 1 ./dist/* ../radweb-starterkit/node_modules/radweb/ && copyfiles -u 1  ./dist/utils/* ../radweb-starterkit/node_modules/radweb/ && copyfiles -u 1  ./dist/utils/angular/* ../radweb-starterkit/node_modules/radweb/ && copyfiles -u 1  ./dist/bundles/* ../radweb-starterkit/node_modules/radweb/ && copyfiles -u 1  ./dist/utils/server/* ../radweb-starterkit/node_modules/radweb/",
    "copyToHugMoms": "copyfiles -u 1 ./dist/* ../hug-moms/node_modules/radweb/ && copyfiles -u 1  ./dist/utils/* ../hug-moms/node_modules/radweb/ && copyfiles -u 1  ./dist/utils/angular/* ../hug-moms/node_modules/radweb/ && copyfiles -u 1  ./dist/bundles/* ../hug-moms/node_modules/radweb/ && copyfiles -u 1  ./dist/utils/server/* ../hug-moms/node_modules/radweb/",
    "copyToIssueTracker": "copyfiles -u 1 ./dist/* ../issue-tracker/node_modules/radweb/ && copyfiles -u 1  ./dist/utils/* ../issue-tracker/node_modules/radweb/ && copyfiles -u 1  ./dist/utils/angular/* ../issue-tracker/node_modules/radweb/ && copyfiles -u 1  ./dist/bundles/* ../issue-tracker/node_modules/radweb/ && copyfiles -u 1  ./dist/utils/server/* ../issue-tracker/node_modules/radweb/",
    "publish-to-npm": "npm run build && npm run addVersion && npm run copyPackage && npm run package && cd dist && npm publish",
    "testPub": "npm run build && npm run copyPackage && npm run package && npm run copyToHugMoms"
  },
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "bootstrap3": "^3.3.5",
    "core-js": "^2.4.1",
    "ngx-bootstrap": "^2.0.3",
    "pg": "^7.4.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.0.39",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/mssql": "^4.0.6",
    "@types/node": "~6.0.60",
    "@types/pg": "^7.4.5",
    "body-parser": "^1.18.2",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "copyfiles": "^1.2.0",
    "express": "^4.16.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mssql": "^4.1.0",
    "nodemon": "^1.12.1",
    "protractor": "~5.1.2",
    "reflect-metadata": "^0.1.10",
    "rollup": "^0.50.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "uglify-js": "^3.1.6"
  }
}
